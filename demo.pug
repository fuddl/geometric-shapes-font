doctype html
html
  head
    style.
      @import 'https://necolas.github.io/normalize.css/5.0.0/normalize.css';
      @font-face {
        font-family: 'Geometric Shapes';
        src: url('../dist/Geometric Shapes.woff');
      }

      body {
        font-family: 'Geometric Shapes', sans-serif;
        font-variant-ligatures: discretionary-ligatures;
        margin: 1em;
        font-size: 1.4em;
      }
      .demo {
        font-size: 3em;
        text-align: center;
      }
      th, td {
        border: 1px solid;
        padding: 1vw;
      }
      .nope {
        opacity: .5;
        text-align: center;
      }
      table {
        border-collapse: collapse;
        margin: 1em 0;
        width: 100%;
      }
      tbody th:first-child {
        text-align: right;
      }
      td {
        text-align: center;
      }
      .glyph-list > figure {
        font-size: 10vw;
        text-align: center;
        width: 20vw;
        margin: 0;
        padding: 0;
      }
      .glyph-list figcaption {
        font-size: .5rem;
      }
      .glyph-list {
        display: flex;
        flex-wrap: wrap;
        justify-content: space-around;
      }
      .reserved {
        background-image: url("data:image/svg+xml,<svg xmlns='http://www.w3.org/2000/svg' width='8' height='8'><path fill='#fff' d='M0 0h8v8h-8z'/><path d='M0 0l8 8zm8 0l-8 8z' stroke-width='.5' stroke='#aaa'/></svg>");
        background-repeat: repeat;
      }



